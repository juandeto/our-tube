generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}


model VideoList {
  id String @id @db.Uuid
  date DateTime @default(now())
  status String @default("PENDING")
  subject String? @db.VarChar
  title String @db.VarChar
  updatedAt DateTime @default(now())
}
